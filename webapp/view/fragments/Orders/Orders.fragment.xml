	<Toolbar width="100%" height="4rem" class="sapUiTinyMarginBottom" >
					<!-- <Button icon="sap-icon://cart-full" class="ordersIcons" type="Transparent" enabled="false" ></Button> -->
					<core:Icon src="sap-icon://cart" class="orderItem sapUiTinyMarginTop sapUiTinyMarginBegin" ></core:Icon>
					<Text text="Orders" class="sapUiTinyMarginBegin sapUiTinyMarginTop Orders_PageTitle" ></Text>
					<ToolbarSpacer></ToolbarSpacer>
						<ComboBox id="dateFilterComboBox" class="create-ship-req_input sapUiTinyMarginEnd" 
							placeholder="Select Date Filter"
							items="{eshipjetModel>/ordersFilterData}"
							selectedKey="{dateFilterModel>/ordersSelectedFilter}"
							selectionChange="onOrdersDateFilterChange" >
							<core:Item key="{eshipjetModel>key}" text="{eshipjetModel>text}" />
						</ComboBox>
					<Button  icon="sap-icon://decline" press="onOrdersClosePress" class="shipNowCloseBtnStyle" type="Transparent" />
				</Toolbar>


 

				<!-- <Toolbar width="100%" height="4rem" class="toolbarbottomborder ordersDarkThemeToolbar">
					<Text text="Orders" class="sapUiTinyMarginBegin sapUiTinyMarginTop Orders_PageTitle ordersTextColor"></Text>
					<ToolbarSpacer></ToolbarSpacer>
					<ComboBox id="dateFilterComboBox" class="create-ship-req_input sapUiTinyMarginEnd" 
						placeholder="Select Date Filter"
						items="{eshipjetModel>/ordersFilterData}"
						selectedKey="{dateFilterModel>/ordersSelectedFilter}"
						selectionChange="onOrdersDateFilterChange" >
						<core:Item key="{eshipjetModel>key}" text="{eshipjetModel>text}" />
					</ComboBox>
				</Toolbar> -->


<!-- <Panel expandable="true" expanded="true" width="auto" >
		<content> -->
		
		
		
		<VBox class="toolbarWrapper">

			<!-- STATUS TOOLBAR -->
			<Toolbar
				class="whiteBackground expandableToolbar"
				height="{= ${eshipjetModel>/isExpandOrdersToolBar} ? '8rem' : '0rem' }"
				visible="{eshipjetModel>/isExpandOrdersToolBar}">

				<HBox width="100%" class="sapUiTinyMargin" >

					<VBox class="sapUiTinyMargin" width="25%">
						<Text text="{eshipjetModel>/allOrdersLength}" class="NumberSizeTitle1"></Text>
						<Text text="Total" class="sapUiTinyMarginBottom"></Text>
						<Button id="Total" press="onOrderStstusBtnFilterPress" class="total-request-button request-button" icon="false" width="100%"></Button>
					</VBox>

					<VBox class="sapUiTinyMargin" width="25%">
						<Text text="{eshipjetModel>/billedCount}" class="NumberSizeTitle1"></Text>
						<Text text="Billed" class="sapUiTinyMarginBottom"></Text>
						<Button id="idBilled" press="onOrderStstusBtnFilterPress" class="billed-request-button request-button" icon="false" width="100%" ></Button>
					</VBox>

					<VBox class="sapUiTinyMargin" width="25%">
						<Text text="{eshipjetModel>/pickUpCount}" class="NumberSizeTitle1"></Text>
						<Text text="Picked-Up" class="sapUiTinyMarginBottom"></Text>
						<Button id="idPickUp" press="onOrderStstusBtnFilterPress" class="pickedUp-request-button request-button" icon="false" width="100%" ></Button>
					</VBox>

					<VBox class="sapUiTinyMargin" width="25%">
						<Text text="{eshipjetModel>/InTransistCount}" class="NumberSizeTitle1"></Text>
						<Text text="In-Transit" class="sapUiTinyMarginBottom"></Text>
						<Button press="onOrderStstusBtnFilterPress" id="In-Transit" class="inTransist-request-button request-button" icon="false" width="100%" ></Button>
					</VBox>

					
					<VBox class="sapUiTinyMargin" width="25%">
						<Text text="{eshipjetModel>/delayedCount}" class="NumberSizeTitle1"></Text>
						<Text text="Delayed" class="sapUiTinyMarginBottom"></Text>
						<Button id="Shipped" press="onOrderStstusBtnFilterPress" class="delayed-request-button request-button" icon="false" width="100%" ></Button>
					</VBox>
					
					<VBox class="sapUiTinyMargin" width="25%">
						<Text text="{eshipjetModel>/deliveredCount}" class="NumberSizeTitle1"></Text>
						<Text text="Delivered" class="sapUiTinyMarginBottom"></Text>
						<Button id="Delivered" press="onOrderStstusBtnFilterPress" class="delivered-request-button request-button" icon="false" width="100%" ></Button>
					</VBox>

					<VBox class="sapUiTinyMargin" width="25%">
						<Text text="{eshipjetModel>/cancelledCount}" class="NumberSizeTitle1"></Text>
						<Text text="Cancelled" class="sapUiTinyMarginBottom"></Text>
						<Button id="Cancelled" press="onOrderStstusBtnFilterPress" class="cancelled-request-button request-button" icon="false" width="100%" ></Button>
					</VBox>
					

				</HBox>
			</Toolbar>

			<!-- EXPAND / COLLAPSE ARROW -->
			<Button
				type="Ghost"
				press="onOrdersToggleExpand"
				icon="{= ${eshipjetModel>/isExpandOrdersToolBar} ? 'sap-icon://slim-arrow-up' : 'sap-icon://slim-arrow-down' }"
				class="expandCollapseArrow"/>
		</VBox>

		<!-- </content>
	</Panel> -->


	<VBox class="whiteBackground sapUiTinyMarginTop" >
		<Toolbar class="sapUiTinyMarginTop sapUiSmallMarginBegin sapUiSmallMarginEnd" >
			<Button icon="sap-icon://filter" press="onOrdersFilterPopoverPress" type="Transparent" class="ordersIcons" ></Button>
				<ToolbarSpacer ></ToolbarSpacer>
				   <SearchField width="10rem" class="sapUiSmallMarginBottom forsearchhigh" value="{eshipjetModel>/ordersSearch}" search="onSearchOrder" ></SearchField>
					<Button icon="sap-icon://synchronize" press="getOrdersHistoryShipments" type="Transparent" class="ordersIcons" ></Button>
					<Button icon="sap-icon://action" press="onOrderExportToExcel" type="Transparent" class="ordersIcons" ></Button>
                    <Button icon="sap-icon://action-settings" press="openOrderColNamesPopover" type="Transparent" class="ordersIcons" ></Button>
                <customData>
                    <core:CustomData key="remember" value="true" />
                    <core:CustomData key="showClearButton" value="true" />
                    <core:CustomData key="confirmButtonText" value="Ok" />
                </customData>
		</Toolbar>

						<ui:Table
							id="idOrdersTable"
							rows="{eshipjetModel>/allOrders}"
							class="sapUiSmallMarginBegin sapUiSmallMarginEnd rowRightLine ordersTableRightBorder "
							selectionMode="None"
							rowHeight="45"
							visibleRowCount="12" >
										<ui:Column  sortProperty="Plant" class="whiteBackground" visible="{eshipjetModel>/OrderTableData/OrderColumns/0/visible}">
									<Label text="Plant ID" />
									<ui:template>
										<Text text="{eshipjetModel>Plant}" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="Vbeln" visible="{eshipjetModel>/OrderTableData/OrderColumns/1/visible}">
									<Label text="Order ID" />
									<ui:template>
										<Text text="{eshipjetModel>Vbeln}" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="Packagetype" visible="{eshipjetModel>/OrderTableData/OrderColumns/2/visible}">
									<Label text="Order Type" />
									<ui:template>
										<Text text="SAP Delivery Number" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="Carriertype" visible="{eshipjetModel>/OrderTableData/OrderColumns/3/visible}">
									<Label text="Ship Method" />
									<ui:template>
										<!-- <HBox justifyContent="SpaceBetween" width="100%" > -->
											<Text text="{eshipjetModel>Carriertype}" wrapping="false" />
											<!-- <Button icon="sap-icon://add" type="Transparent" ></Button> -->
										<!-- </HBox> -->
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="CarrierCode" visible="{eshipjetModel>/OrderTableData/OrderColumns/4/visible}">
									<Label text="ERP Service ID" />
									<ui:template>
										<Text text="{eshipjetModel>Carriertype}" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="CarrierCode" visible="{eshipjetModel>/OrderTableData/OrderColumns/5/visible}">
									<Label text="Ship Type" />
									<ui:template>
										<Text text="{eshipjetModel>CarrierDesc}" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="CarrierDesc" visible="{eshipjetModel>/OrderTableData/OrderColumns/6/visible}">
									<Label text="Service Name" />
									<ui:template>
										<Text text="{eshipjetModel>Shipmentid}" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="ShipmentType" visible="{eshipjetModel>/OrderTableData/OrderColumns/7/visible}">
									<Label text="Type" />
									<ui:template>
										<Text text="{eshipjetModel>ShipmentType}" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="Totalpkg" visible="{eshipjetModel>/OrderTableData/OrderColumns/8/visible}">
									<Label text="Pkgs" />
									<ui:template>
									<HBox justifyContent="Start" width="100%" >
										<Text text="{= parseInt(${eshipjetModel>Totalpkg}, 10) }" wrapping="false" />
									</HBox>
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="PGI" visible="{eshipjetModel>/OrderTableData/OrderColumns/9/visible}">
								<Label text="PGI" />
								<ui:template>
									<Button 
										icon="{= ${eshipjetModel>eShipjetPGIStatus} === 'S' ? 'sap-icon://accept' : 'sap-icon://decline'}" 
										type="{= ${eshipjetModel>eShipjetPGIStatus} === 'S' ? 'Success' : 'Error'}" /> 
										<Button icon="{= 
													${eshipjetModel>eShipjetPGIStatus} === 'S' ? 'sap-icon://accept' : 
													${eshipjetModel>eShipjetPGIStatus} === 'E' ? 'sap-icon://decline' : ''
												}"
												enabled="false"
											class="greenColorForIcon ordersSmallText"
											type="Transparent" />

											<!-- <core:Icon
											visible="{= ${eshipjetModel>eShipjetPGIStatus} === 'S' || 
													${eshipjetModel>eShipjetPGIStatus} === 'F' }"

											src="{= ${eshipjetModel>eShipjetPGIStatus} === 'S'
													? 'sap-icon://accept'
													: 'sap-icon://decline' }"

											color="{= ${eshipjetModel>eShipjetPGIStatus} === 'S'
													? '#107E3E'
													: '#BB0000' }"
										/> -->

								</ui:template>
							</ui:Column>


								<ui:Column  sortProperty="DateAdded" visible="{eshipjetModel>/OrderTableData/OrderColumns/10/visible}">
									<Label text="Ship Date-Time" />
									<ui:template>
										<!-- <Text text="{path: 'eshipjetModel>LastChangedOn', formatter: '.formatter.formatCustomDateTimeForOrders'}" wrapping="false" /> -->
										<Text text="{path: 'eshipjetModel>LastChangedOn' }" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="TrackingNumber" visible="{eshipjetModel>/OrderTableData/OrderColumns/11/visible}">
									<Label text="Tracking/Pro #" />
									<ui:template>
										<Link text="{eshipjetModel>TrackingNumber}" press="onTrackingNumberPress" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column sortProperty="DeliveryStatus" visible="{eshipjetModel>/OrderTableData/OrderColumns/12/visible}">
								<Label text="Status" />
								<ui:template>
									<Text text="Shipped"  wrapping="false" class="greenColorForIcon " /> 
									<!-- <Text text="{path: 'eshipjetModel>Shipprocess', formatter: '.formatter.formatShipmentType'}" wrapping="false" class="greenColorForIcon ordersSmallText" /> -->
								</ui:template>
								</ui:Column>
								<ui:Column  sortProperty="ExpDelDate" visible="{eshipjetModel>/OrderTableData/OrderColumns/13/visible}">
									<Label text="Del'd Date-Time" />
									<ui:template>
										<Text text="{path: 'eshipjetModel>DateAdded', formatter: '.formatter.formatCustomDateTimeForOrders'}" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="RecContact" visible="{eshipjetModel>/OrderTableData/OrderColumns/14/visible}">
									<Label text="Ship To Contact" />
									<ui:template>
										<Text text="{eshipjetModel>RecContact}" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="RecCompany" visible="{eshipjetModel>/OrderTableData/OrderColumns/15/visible}">
									<Label text="Ship To Company" />
									<ui:template>
										<Text text="{eshipjetModel>RecCompany}" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="RecCity" visible="{eshipjetModel>/OrderTableData/OrderColumns/16/visible}">
									<Label text="Ship To City" />
									<ui:template>
										<Text text="{eshipjetModel>RecCity}" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="RecRegion" visible="{eshipjetModel>/OrderTableData/OrderColumns/17/visible}">
									<Label text="Ship To State" />
									<ui:template>
										<Text text="{eshipjetModel>RecRegion}" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="RecPostalcode" visible="{eshipjetModel>/OrderTableData/OrderColumns/18/visible}">
									<Label text="Ship To Postal" />
									<ui:template>
										<Text text="{eshipjetModel>RecPostalcode}" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="RecCountry" visible="{eshipjetModel>/OrderTableData/OrderColumns/19/visible}">
									<Label text="Ship To Country" />
									<ui:template>
										<Text text="{eshipjetModel>RecCountry}" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="RecPhone" visible="{eshipjetModel>/OrderTableData/OrderColumns/20/visible}">
									<Label text="Ship To Phone" />
									<ui:template>
										<Text text="{eshipjetModel>RecPhone}" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="Emailaddress" visible="{eshipjetModel>/OrderTableData/OrderColumns/21/visible}">
									<Label text="Ship To Email" />
									<ui:template>
										<Text text="{eshipjetModel>Emailaddress}" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column width="{= ${eshipjetModel>/orderColSelectedCount} >= 15 ? '5rem' : 'auto' }" sortProperty="" visible="{eshipjetModel>/OrderTableData/OrderColumns/22/visible}">
									<Label text="User ID" />
									<ui:template>
										<Text text="{eshipjetModel>userName}" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column sortProperty="" visible="{eshipjetModel>/OrderTableData/OrderColumns/23/visible}">
									<Label text="SAP User ID" />
									<ui:template>
										<Text text="{eshipjetModel>userName}" wrapping="false" />
									</ui:template>
								</ui:Column>

								<ui:Column  sortProperty="" visible="{eshipjetModel>/OrderTableData/OrderColumns/24/visible}" hAlign="Center" >
									<Label text="Note" />
									<ui:template>
										<VBox justifyContent="Center" alignItems="Center">
												<Button icon="sap-icon://document-text" press="onShipmentWorkflowPress"  type="Transparent" ></Button>
										</VBox>
									</ui:template>
								</ui:Column>

								<ui:Column width="{= ${eshipjetModel>/orderColSelectedCount} >= 15 ? '7rem' : 'auto' }" hAlign="End" >
									<Label text="Actions"/>
									<ui:template>
										<HBox>
											<Button text="View Now" class="ordersSmallButton" type="Transparent" press="onViewNowPressBackToShipNow" ></Button>
											<Button icon="sap-icon://megamenu" class="ordersSmallButton" type="Transparent" press="handleDownArrowPress" ></Button>
										</HBox>
									</ui:template>
								</ui:Column>
						</ui:Table> 
	</VBox>


								<Toolbar class="whiteBackground sapUiTinyMarginTop ordersTableFooter">
									<ToolbarSpacer/>
									<Button text="First" type="Transparent" press=".onFirstPage"
										enabled="{eshipjetModel>/FirstBtnVisableStatus}" />
									<Button text="Previous" type="Transparent" press=".onPreviousPage"
										enabled="{eshipjetModel>/PreviousBtnVisableStatus}" />
									<Text id="paginationText" text="1-50 of 0" />
									<Button text="Next" type="Transparent" press=".onNextPage"
										enabled="{eshipjetModel>/NextBtnVisableStatus}" />
									<Button text="Last" type="Transparent" press=".onLastPage"
										enabled="{eshipjetModel>/LastBtnVisableStatus}" />
								</Toolbar>





