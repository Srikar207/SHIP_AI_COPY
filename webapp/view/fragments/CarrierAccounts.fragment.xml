		<Toolbar width="auto" class="carrierCatalogToolbar noBorderToolbar" height="5rem" >
			<Text text="Carrier Account Configuration" class="toolBarHeader sapUiTinyMarginBegin whiteColor" ></Text>
				<ToolbarSpacer />
					<HBox>
						<Button  text="Add Carrier Account" press="AddCarrierConfigurationDialogPress" class="addCarrierBtnStyle" width="auto" ></Button>
						<!-- <Button type="Success" press="onCarrierAccountsArrowPress" icon="sap-icon://navigation-down-arrow" class="Master-toolbar__button sapUiTinyMarginEnd" ></Button> -->
                	</HBox>
		</Toolbar>

        <Toolbar height="25%" class="toolbarbottomborder" >
			<HBox width="100%" >
				<VBox class="sapUiTinyMargin" width="33%">
					<Text text="{eshipjetModel>/totalCarriersAcccountsCount}" class="NumberSizeTitle1"></Text>
					<Text text="Total Carrier Accounts" class="NumberSizeTitle2"></Text>
					<Button class="totalProgress-request-button request-button btnInnerFocus" icon="false" width="100%"  ></Button>
				</VBox>
				<VBox class="sapUiTinyMargin" width="33%">
					<Text text="{eshipjetModel>/activeCarriersAcccountsCount}" class="NumberSizeTitle1"></Text>
					<Text text="Active" class="NumberSizeTitle2"></Text>
					<Button class="inTransit-request-button request-button btnInnerFocus" icon="false" width="100%" ></Button>
				</VBox>
				<VBox class="sapUiTinyMargin" width="33%">
					<Text text="{eshipjetModel>/inActiveCarriersAcccountsCount}" class="NumberSizeTitle1"></Text>
					<Text text="Inactive" class="NumberSizeTitle2"></Text>
					<Button class="InWhite-request-button request-button btnInnerFocus" icon="false" width="100%" ></Button>
				</VBox>
			</HBox>
		</Toolbar>

	<VBox class="whiteBackgroundForTables sapUiTinyMarginTop" >
		<Toolbar class="sapUiTinyMarginTop sapUiSmallMarginBegin sapUiSmallMarginEnd" >
			<Button icon="sap-icon://filter" press="onCarriesAccountsFilterPopoverPress" type="Transparent" class="ordersIcons" ></Button>
			<ToolbarSpacer ></ToolbarSpacer>
				<SearchField width="10rem" class="sapUiSmallMarginBottom" liveChange="onSearchCarrierAccounts" ></SearchField>
				<Button icon="sap-icon://action" press="onCarrierAccountsExportToExcel" type="Transparent" class="ordersIcons" ></Button>
				<Button icon="sap-icon://action-settings" press="openCarrierAccountColNamesPopover" type="Transparent" class="ordersIcons" ></Button>
			<customData>
				<core:CustomData key="remember" value="true" />
				<core:CustomData key="showClearButton" value="true" />
				<core:CustomData key="confirmButtonText" value="Ok" />
			</customData>
		</Toolbar>
		<ui:Table	
			id="_IDCarriesAccountsTable"
			selectionMode="None"
			class="rowRightLine ordersTableRightBorder"
			rows="{eshipjetModel>/uniqueCarriersAcccountsData}"
			visibleRowCount="13"
			ariaLabelledBy="title">

					<ui:columns>
								<ui:Column visible="{eshipjetModel>/CarrierAccountsTableColumns/0/visible}" >
									<Label text="Location ID" />
									<ui:template>
										<Text text="{eshipjetModel>LocationId}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column visible="{eshipjetModel>/CarrierAccountsTableColumns/1/visible}" width="auto" >
									<Label text="Carrier Name" />
									<ui:template>
										<Text text="{eshipjetModel>CarrierCode}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column visible="{eshipjetModel>/CarrierAccountsTableColumns/2/visible}" >
									<Label text="Carrier Type" />
									<ui:template>
										<Text text="{eshipjetModel>CarrierType}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column visible="{eshipjetModel>/CarrierAccountsTableColumns/3/visible}" >
									<Label text="Connection Type" />
									<ui:template>
										<Text text="{eshipjetModel>ConnectionType}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column visible="{eshipjetModel>/CarrierAccountsTableColumns/4/visible}" >
									<Label text="Ship From Country" />
									<ui:template>
										<Text text="{eshipjetModel>ShipFrmCntry}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column visible="{eshipjetModel>/CarrierAccountsTableColumns/5/visible}" >
									<Label text="Ship To Country" />
									<ui:template>
										<Text text="{eshipjetModel>ShipToCntry}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column visible="{eshipjetModel>/CarrierAccountsTableColumns/6/visible}" >
									<Label text="Ship URL" />
									<ui:template>
										<Text text="{eshipjetModel>ShipUrl}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column visible="{eshipjetModel>/CarrierAccountsTableColumns/7/visible}" >
									<Label text="Void URL" />
									<ui:template>
										<Text text="{eshipjetModel>VoidUrl}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column visible="{eshipjetModel>/CarrierAccountsTableColumns/8/visible}" >
									<Label text="Rate URL" />
									<ui:template>
										<Text text="{eshipjetModel>RateUrl}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column visible="{eshipjetModel>/CarrierAccountsTableColumns/9/visible}" >
									<Label text="Track URL" />
									<ui:template>
										<Text text="{eshipjetModel>TrackUrl}" wrapping="false" />
									</ui:template>
								</ui:Column>
								<ui:Column visible="{eshipjetModel>/CarrierAccountsTableColumns/10/visible}" >
									<Label text="Freight Quote" />
									<ui:template>
										<Switch  state="{eshipjetModel>FreightQuote}" class="greenSwitch">
											<layoutData>
												<FlexItemData growFactor="1" />
											</layoutData>
										</Switch>
									</ui:template>
								</ui:Column>
								<ui:Column visible="{eshipjetModel>/CarrierAccountsTableColumns/11/visible}" >
									<Label text="Rate Quote" />
									<ui:template>
										<Switch  state="{eshipjetModel>RateQuote}" class="greenSwitch">
											<layoutData>
												<FlexItemData growFactor="1" />
											</layoutData>
										</Switch>
									</ui:template>
								</ui:Column>
								<ui:Column visible="{eshipjetModel>/CarrierAccountsTableColumns/12/visible}" >
									<Label text="Automate Pro Number" />
									<ui:template>
										<Switch  state="{eshipjetModel>AutomateProNumber}" class="greenSwitch">
											<layoutData>
												<FlexItemData growFactor="1" />
											</layoutData>
										</Switch>
									</ui:template>
								</ui:Column>
								<ui:Column visible="{eshipjetModel>/CarrierAccountsTableColumns/13/visible}" >
									<Label text="Status" />
									<ui:template>
										<Switch  state="{eshipjetModel>IsActive}" class="greenSwitch" >
											<layoutData>
												<FlexItemData growFactor="1" />
											</layoutData>
										</Switch>
									</ui:template>
								</ui:Column>
								<ui:Column visible="{eshipjetModel>/CarrierAccountsTableColumns/14/visible}" >
									<Label text="End Of The Day" />
									<ui:template>
										<Switch state="{eshipjetModel>EndOfDay}" class="greenSwitch">
											<layoutData>
												<FlexItemData growFactor="1" />
											</layoutData>
										</Switch>
									</ui:template>
								</ui:Column>
								<ui:Column visible="{eshipjetModel>/CarrierAccountsTableColumns/15/visible}" >
									<Label text="Manual Rates" />
									<ui:template>
										<Switch state="{eshipjetModel>ManualRates}" class="greenSwitch">
											<layoutData>
												<FlexItemData growFactor="1" />
											</layoutData>
										</Switch>
									</ui:template>
								</ui:Column>
								<ui:Column visible="true" >
									<Label text="Actions" />
									<ui:template>
										<HBox >
											<Button text="Edit" type="Transparent" press="onEditCarrierAccountPress" ></Button>
											<Button icon="sap-icon://megamenu" type="Transparent" ></Button>
										</HBox>
									</ui:template>
								</ui:Column>
							</ui:columns>
		</ui:Table>
    </VBox>

	<Toolbar class="whiteBackgroundForTables ordersTableFooter">
		<ToolbarSpacer/>
			<Button text="First" icon="sap-icon://close-command-field" class="btnInnerFocus" />
			<Button text="Previous" icon="sap-icon://nav-back" class="btnInnerFocus" />
			<Text text="1-0 of 0" />
			<Button text="Next" class="btnInnerFocus" />
			<Button text="Last" icon="sap-icon://open-command-field" class="btnInnerFocus" iconFirst="false" />
	</Toolbar>
    

</core:FragmentDefinition>
