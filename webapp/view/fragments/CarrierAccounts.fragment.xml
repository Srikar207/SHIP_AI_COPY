		<Toolbar width="auto" class="carrierCatalogToolbar noBorderToolbar" height="5rem" >
			<Text text="Carrier Account Configuration" class="toolBarHeader sapUiTinyMarginBegin whiteColor" ></Text>
				<ToolbarSpacer />
					<HBox>
						<Button  text="Add Carrier Account" press="AddCarrierConfigurationDialogPress" class="addCarrierBtnStyle" width="auto" ></Button>
						<!-- <Button type="Success" press="onCarrierAccountsArrowPress" icon="sap-icon://navigation-down-arrow" class="Master-toolbar__button sapUiTinyMarginEnd" ></Button> -->
                	</HBox>
		</Toolbar>

        <Toolbar height="25%" class="toolbarbottomborder" >
			<HBox width="100%" >
				<VBox class="sapUiTinyMargin" width="33%">
					<Text text="1" class="NumberSizeTitle1"></Text>
					<Text text="Total Carrier Accounts" class="NumberSizeTitle2"></Text>
					<Button class="totalProgress-request-button request-button btnInnerFocus" icon="false" width="100%"  ></Button>
				</VBox>
				<VBox class="sapUiTinyMargin" width="33%">
					<Text text="2" class="NumberSizeTitle1"></Text>
					<Text text="Active" class="NumberSizeTitle2"></Text>
					<Button class="inTransit-request-button request-button btnInnerFocus" icon="false" width="100%" ></Button>
				</VBox>
				<VBox class="sapUiTinyMargin" width="33%">
					<Text text="3" class="NumberSizeTitle1"></Text>
					<Text text="Inactive" class="NumberSizeTitle2"></Text>
					<Button class="InWhite-request-button request-button btnInnerFocus" icon="false" width="100%" ></Button>
				</VBox>
			</HBox>
		</Toolbar>

	<VBox class="whiteBackgroundForTables sapUiTinyMarginTop" >
		<Toolbar class="sapUiTinyMarginTop sapUiSmallMarginBegin sapUiSmallMarginEnd" >
			<Button icon="sap-icon://filter" press="onCarriesAccountsFilterPopoverPress" type="Transparent" class="ordersIcons" ></Button>
			<ToolbarSpacer ></ToolbarSpacer>
				<SearchField width="10rem" class="sapUiSmallMarginBottom forsearchhigh" liveChange="onSearchCarrierAccounts" ></SearchField>
				<Button icon="sap-icon://action" press="onCarrierAccountsExportToExcel" type="Transparent" class="ordersIcons" ></Button>
				<Button icon="sap-icon://action-settings" press="openCarrierAccountColNamesPopover" type="Transparent" class="ordersIcons" ></Button>
			<customData>
				<core:CustomData key="remember" value="true" />
				<core:CustomData key="showClearButton" value="true" />
				<core:CustomData key="confirmButtonText" value="Ok" />
			</customData>
		</Toolbar>
		<ui:Table	
			id="_IDCarriesAccountsTable"
			selectionMode="None"
			class="rowRightLine ordersTableRightBorder"
			visibleRowCount="13"
			ariaLabelledBy="title">
		</ui:Table>
    </VBox>

	<Toolbar class="whiteBackgroundForTables ordersTableFooter">
		<ToolbarSpacer/>
			<Button text="First" icon="sap-icon://close-command-field" class="btnInnerFocus" />
			<Button text="Previous" icon="sap-icon://nav-back" class="btnInnerFocus" />
			<Text text="1-0 of 0" />
			<Button text="Next" class="btnInnerFocus" />
			<Button text="Last" icon="sap-icon://open-command-field" class="btnInnerFocus" iconFirst="false" />
	</Toolbar>
    

</core:FragmentDefinition>
