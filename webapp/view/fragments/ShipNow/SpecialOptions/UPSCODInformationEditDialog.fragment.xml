<core:FragmentDefinition
    xmlns:core="sap.ui.core" 
    xmlns:f="sap.ui.layout.form"
    xmlns:mvc="sap.ui.core.mvc"
	xmlns:l="sap.ui.layout"
	xmlns="sap.m">

    <Dialog id="_IDGenUPSCODInformationEditDialog"
            title="CODInformation Information"
            contentWidth="60vh"
            contentHeight="95%"
            verticalScrolling="false"
            horizontalScrolling="false"
            class="rightSideDialogSpecialOptions">
            
            <Toolbar>
                <Title text="CODInformation Information" />
                <ToolbarSpacer />
                <Button icon="sap-icon://decline" press="onCloseUPSCODInformationEditDialog" />
            </Toolbar>

        <content>
            <f:SimpleForm editable="true"
                          class="customSimpleFormShadow"
                          width="auto"
                          layout="ColumnLayout"
                          columnsM="2"
                          columnsL="2"
                          columnsXL="2">
                <f:content>
                   <!-- <Toolbar ariaLabelledBy="idShipFromsdsddd" class="customLine" >
						<Button icon="sap-icon://customer-briefing" id="idShipFrggomttAddressBtn" />
                        <Title id="idShipFdfgdromuuqwttq" text="Contact Information" />
					</Toolbar> -->
                    <Label text="code" />
                    <ComboBox class="sapUiSmallMarginBottom create-ship-req_input"></ComboBox>
                </f:content>
            </f:SimpleForm>
        </content>

        <footer>
            <OverflowToolbar class="sapMPopoverFooter1">
                <Button text="Cancel" press="onCloseUPSCODInformationEditDialog" />
                <ToolbarSpacer />
                <Button text="Save" type="Emphasized" press="onCloseUPSCODInformationEditDialog" />
            </OverflowToolbar>
        </footer>

    </Dialog>
</core:FragmentDefinition>
