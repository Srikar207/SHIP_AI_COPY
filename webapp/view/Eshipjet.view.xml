					<ScrollContainer	id="TrackNow" horizontal="false" vertical="true" height="calc(100% - 50px)" class="sapUiContentPadding">
						<core:Fragment fragmentName="com.eshipjet.zeshipjet.view.fragments.TrackNow.TrackNow" type="XML" />
